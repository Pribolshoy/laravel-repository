{
    "name": "pribolshoy/laravel-repository",
    "description": "Laravel repository pattern implementation",
    "keywords": ["laravel", "repository", "eloquent"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nikolay Pribolshoy",
            "email": "pribolshoy@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "pribolshoy/repository": "^1.0",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/cache": "^10.0|^11.0",
        "illuminate/redis": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "mockery/mockery": "2.0.x-dev"
    },
    "autoload": {
        "psr-4": {
            "pribolshoy\\laravelrepository\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    }
}
